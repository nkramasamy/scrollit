{
    "name": "ScrollIt",
    "description": "Never scroll again.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["node_modules/jquery/dist/jquery.js", "content.js"],
        "all_frames":true
      }
    ]
}