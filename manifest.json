{
    "name": "ScrollIt",
    "description": "Never scroll again.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["node_modules/jquery/dist/jquery.js", "content.js"],
          "all_frames":true
        }
      ]
  }